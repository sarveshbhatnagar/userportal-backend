  "license": "None",
  "bugs": {
    "url": "https://github.com/sarveshbhatnagar/userportal-backend/issues"
  },
  "homepage": "https://github.com/sarveshbhatnagar/userportal-backend#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "esbuild": "^0.17.5",
    "jest": "^29.4.2"
  },
  "dependencies": {
    "aws-sdk": "^2.1310.0",
    "bcryptjs": "^2.4.3",
    "lambda-build": "^1.0.6",
    "uuid": "^9.0.0"
  }
